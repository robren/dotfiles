
# User specific aliases and functions
alias st2='sublime'
alias rm='rm -i'
alias cp='cp -i'
alias mv='mv -i'

alias h='history'
alias j='jobs -l'
alias which='type -a'
alias p='pushd'
alias o='popd'

alias ..='cd ..'
alias ...='cd ../..'
alias ....='cd ../../..'

# some more ls aliases
alias new='ls -lat |head'
alias ll='ls -alF'
alias la='ls -A'
alias l='ls -CF'

